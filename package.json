  },
  "homepage": "https://github.com/shubhamidsharma/ProShop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^16.0.1",
    "express-async-handler": "^1.2.0",
    "express.js": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.5",
    "moongoose": "^0.0.5",
    "moongose": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
